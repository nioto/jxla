<?xml version="1.0" encoding="ISO-8859-1"?>

<jxla>

  <dns>
    <available>false</available>
    <filecache>D:/tmp/logs/dns.dump</filecache>
  </dns>

  <logfiles>
    <directory>D:/tmp/logs/</directory>
    <filenameregexp>(.*)\.log</filenameregexp>
    <format>
      <regexp>$remote_host - $user [d/m/y*h *] "* $uri *" $status $size $referer $agent*</regexp>
      <regexp>$remote_ip - $user [d/m/y*h *] "* $uri *" $status $size $referer $agent*</regexp>
      <regexp>m d h * * y $host $status $remote_ip "* $uri *" * $agent $lang $referer *</regexp>
      <regexp>m d h * * y $host $status $remote_ip "* $uri *" * $lang $referer "*" * *</regexp>
      <regexp>m d h * * y $host $status $remote_ip "* $uri *" $agent $lang $referer "$user" "*" $size *</regexp>
      <regexp>y-m-d h $remote_ip * $host * * $uri * $status $agent</regexp>
    </format>
  </logfiles>

  <pages>
    <extensions>.xml, .htm, .html, .php, .php, .php4, .pl, .cgi, .dll</extensions>
    <defaultindex>index.html</defaultindex>
  </pages>

  <max-values>
    <referers>20</referers>
    <keywords>50</keywords>
    <remote-hosts>25</remote-hosts>
    <uri>30</uri>
    <notfound>10</notfound>
    <countries>1000</countries>
    <browsers>50</browsers>
    <operating-system>50</operating-system>
  </max-values>

  <localconfigclass>org.novadeck.jxla.config.SimpleSiteConfig</localconfigclass>

  <summary-name>summary.xml</summary-name>


  <history-filepath>D:/tmp/logs/sites.hist</history-filepath>

  <searchengines>
    <engine>
      <name>Yahoo!</name>
      <requestparameter>p</requestparameter>
      <domain>yahoo.fr</domain>
    </engine>
    <engine>
      <name>Yahoo!</name>
      <requestparameter>p</requestparameter>
      <domain>yahoo.com</domain>
    </engine>
    <engine>
      <name>gOOgle</name>
      <requestparameter>q</requestparameter>
      <domain>google.fr</domain>
    </engine>
    <engine>
      <name>gOOgle</name>
      <requestparameter>q</requestparameter>
      <domain>google.com</domain>
    </engine>
    <engine>
      <name>NetScape</name>
      <requestparameter>search</requestparameter>
      <domain>netscape.com</domain>
    </engine>
  </searchengines>

</jxla>

